@model IEnumerable<string>

<form name="compare" method="get">
    <div class="row">
        <div class="col-xs-6 col-sm-4">
            <div class="row">
                <div class="col-sm-3 titles">From:</div>
                <div class="col-sm-9">
                    <select name="from">
                        @foreach (var version in Model)
                        {
                            <option name="@version" value="@version">@version</option>
                        }
                    </select>
                </div>
            </div>
        </div>
        <div class="col-xs-6 col-sm-4">
            <div class="row">
                <div class="col-sm-3 titles">To:</div>
                <div class="col-sm-9">
                    <select name="to">
                       @foreach (var version in Model)
                       {
                           <option name="@version" value="@version">@version</option>
                       }
                    </select>
                </div>
            </div>
        </div>
        <div class="col-xs-12 col-sm-4">
            <div class="row">
                <div class="col-xs-12">
                    <input type="submit" value="Compare" class="button" />
                </div>
            </div>
        </div>
    </div>
</form>